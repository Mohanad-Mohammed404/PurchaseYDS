<odoo>
    <data>

        <record id="purchase_request_lines_view_tree" model="ir.ui.view">
            <field name="name">purchase.request.lines.view.tree</field>
            <field name="model">purchase.request.lines</field>
            <field name="arch" type="xml">
                <tree string="purchase_request_lines_tree">
                    <field name="product_id"/>
                    <field name="vendor_id"/>
                    <field name="quantity"/>
                </tree>
            </field>
        </record>

        <record id="purchase_request_lines_view_form" model="ir.ui.view">
            <field name="name">purchase.request.lines.view.form</field>
            <field name="model">purchase.request.lines</field>
            <field name="arch" type="xml">
                <form string="purchase_request_lines_form">
                    <sheet>
                        <group>
                            <group>
                                <field name="product_id"/>
                                <field name="vendor_id"/>
                                <field name="quantity"/>
                            </group>
                            <group>
                                <field name="product_uom"/>
                                <field name="product_uom_category_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>